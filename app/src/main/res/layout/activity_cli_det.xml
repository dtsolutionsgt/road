<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relclimain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@layout/back"
    tools:context=".CliDet">


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_above="@+id/rellaycr"
        android:layout_below="@+id/relRoad"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false">

        <TextView
            android:id="@+id/lblTel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="6dp"
            android:layout_marginBottom="2dp"
            android:layout_toEndOf="@+id/lblTelefono"
            android:gravity="center"
            android:text="0000-0000"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblRuta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/scroll1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:text="Ruta"
            android:textSize="18sp" />


        <ScrollView
            android:id="@+id/scroll1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/txtRuta"
            android:layout_marginEnd="65dp"
            android:layout_marginBottom="5dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/lblClaseSub"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lblClase"
                    android:paddingStart="5dp"
                    android:text="Subcanal"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/lblClase"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lblDir"
                    android:layout_alignParentBottom="false"
                    android:background="#F0F0F0"
                    android:paddingStart="5dp"
                    android:text="Canal"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/lblNom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:paddingLeft="5dp"
                    android:paddingTop="3dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="3dp"
                    android:text="Nombre"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/lblPres"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblNom"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:background="#F0F0F0"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="3dp"
                    android:text="Representante"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/lblDir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/lblPres"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="0dp"
                    android:layout_weight="1"
                    android:width="0dip"
                    android:background="@drawable/frame_btn"
                    android:clickable="true"
                    android:maxLines="2"
                    android:onClick="showDir"
                    android:paddingLeft="5dp"
                    android:paddingTop="5dp"
                    android:paddingRight="5dp"
                    android:paddingBottom="5dp"
                    android:scrollHorizontally="false"
                    android:text="Direccion"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <TextView
                    android:id="@+id/lblDescripcionPago"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lblClaseSub"
                    android:background="#F0F0F0"
                    android:paddingStart="5dp"
                    android:paddingTop="0dp"
                    android:text="Descripcion_pago"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView100"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lblDescripcionPago"
                    android:paddingStart="5dp"
                    android:paddingTop="0dp"
                    android:text="Categorización"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView98"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView100"
                    android:background="#F0F0F0"
                    android:paddingStart="5dp"
                    android:paddingEnd="5dp"
                    android:text="Provincia"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/textView99"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView98"
                    android:paddingStart="5dp"
                    android:paddingEnd="5dp"
                    android:text="Distrito"
                    android:textSize="18sp" />
            </RelativeLayout>
        </ScrollView>


        <TextView
            android:id="@+id/lblCant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="false"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="5dp"
            android:layout_marginBottom="30dp"
            android:layout_toEndOf="@+id/lblProd"
            android:gravity="center_horizontal"
            android:paddingLeft="5dp"
            android:text="01-01-2012"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblProd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="false"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="3dp"
            android:layout_marginBottom="30dp"
            android:gravity="center_horizontal"
            android:text="Últ. aten :"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="18sp" />

        <RelativeLayout
            android:id="@+id/relGPS"
            android:layout_width="160dp"
            android:layout_height="32dp"
            android:layout_alignTop="@+id/lblCant"
            android:layout_alignParentLeft="false"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="false"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="197dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="2dp"
            android:clickable="true"
            android:onClick="setGPS">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="32dp"
                android:layout_height="24dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="2dp"
                android:layout_marginBottom="0dp"
                android:src="@drawable/iconoubicacion" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="1dp"
                android:layout_marginRight="2dp"
                android:layout_marginBottom="0dp"
                android:layout_toRightOf="@+id/imageView3"
                android:gravity="left|center_vertical"
                android:text="Small Text"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="12sp" />

        </RelativeLayout>

        <TextView
            android:id="@+id/lblTelefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="2dp"
            android:layout_marginBottom="2dp"
            android:text="Tel :"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
            android:id="@+id/txtRuta"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="false"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="13dp"
            android:layout_marginBottom="60dp"
            android:layout_toEndOf="@+id/lblRuta"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name" />

        <TextView
            android:id="@+id/lblRuta2"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/txtRuta"
            android:layout_alignBottom="@+id/txtRuta"
            android:layout_alignParentEnd="false"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_toEndOf="@+id/txtRuta"
            android:background="@drawable/frame_btn"
            android:clickable="true"
            android:gravity="center"
            android:onClick="rutaHelp"
            android:text=" ? "
            android:textSize="18sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rellaycr"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_above="@+id/RelativeLayout01"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="-3dp"
        android:background="@layout/frame_rect">

        <TextView
            android:id="@+id/lblCUsed"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="7dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="1dp"
            android:layout_marginRight="0dp"
            android:layout_marginBottom="0dp"
            android:gravity="center_horizontal"
            android:paddingBottom="5dp"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblCDisp"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:gravity="center_horizontal"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:text="000.000.00"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblCLim"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/lblCUsed"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="1dp"
            android:layout_marginRight="131dp"
            android:layout_marginBottom="2dp"
            android:gravity="center_horizontal"
            android:paddingBottom="5dp"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblBU"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblCLim"
            android:layout_alignEnd="@+id/lblCLim"
            android:layout_alignParentStart="true"
            android:layout_toRightOf="@+id/lblFecha"
            android:gravity="center_horizontal"
            android:paddingBottom="5dp"
            android:text="Límite"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/lblPTotal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblCUsed"
            android:layout_alignStart="@+id/lblCUsed"
            android:layout_alignEnd="@+id/lblCUsed"
            android:gravity="center_horizontal"
            android:paddingBottom="5dp"
            android:text="Usado"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/lblCheq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblCDisp"
            android:layout_alignStart="@+id/lblCDisp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp"
            android:gravity="center_horizontal"
            android:paddingBottom="5dp"
            android:text="Disponible "
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/lblClientePago"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblBU"
            android:layout_alignParentStart="true"
            android:layout_marginStart="10dp"
            android:layout_marginBottom="5dp"
            android:text="Contado"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblDiasCredito"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblCheq"
            android:layout_alignStart="@+id/lblCheq"
            android:layout_marginBottom="0dp"
            android:layout_toStartOf="@+id/lblCantDias"
            android:gravity="right"
            android:paddingBottom="5dp"
            android:text="Días :"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/lblCantDias"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:layout_above="@+id/lblCheq"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="4dp"
            android:gravity="right"
            android:paddingRight="10dp"
            android:paddingBottom="5dp"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textStyle="bold" />

    </RelativeLayout>

    <TextView
        android:id="@+id/lblCPago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout1"
        android:layout_alignParentRight="true"
        android:text="Devolucion"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#FFFFFF"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/lblCValor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:gravity="center_horizontal"
        android:text="Cobros"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#FFFFFF"
        android:textSize="10sp" />

    <RelativeLayout
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relRoad"
        android:layout_alignBottom="@+id/relativeLayout1"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true">

        <RelativeLayout
            android:id="@+id/relVenta"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:clickable="true"
            android:onClick="showVenta">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="false"
                android:clickable="false"
                android:onClick="showVenta"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:src="@drawable/factura_redonda" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView4"
                android:layout_alignParentLeft="true"
                android:gravity="center_horizontal"
                android:paddingBottom="5dp"
                android:text="VENTA"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="10sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relPreventa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
            android:layout_below="@+id/relVenta"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="false"
            android:clickable="true"
            android:onClick="showPreventa">

            <ImageView
                android:id="@+id/ImageView01"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="false"
                android:clickable="true"
                android:onClick="showPreventa"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:src="@drawable/pedido_redondo" />

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ImageView01"
                android:layout_alignParentLeft="true"
                android:gravity="center_horizontal"
                android:paddingBottom="5dp"
                android:text="PREVENTA"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="10sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relCanastas"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
            android:layout_below="@+id/relDespacho"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="false"
            android:clickable="true"
            android:onClick="showCanastas">

            <ImageView
                android:id="@+id/ImageCanastas"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="false"
                android:clickable="false"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:src="@drawable/canasta_redonda" />

            <TextView
                android:id="@+id/lblCanastas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ImageCanastas"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:gravity="center_horizontal"
                android:paddingBottom="5dp"
                android:text="CANASTAS"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="10sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relDespacho"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignWithParentIfMissing="true"
            android:layout_below="@+id/relPreventa"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="false"
            android:clickable="true"
            android:onClick="showDespacho">

            <ImageView
                android:id="@+id/imgDespacho"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="false"
                android:clickable="false"
                android:paddingTop="6dp"
                android:paddingBottom="6dp"
                android:src="@drawable/despacho_redondo" />

            <TextView
                android:id="@+id/lblDespacho"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgDespacho"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:gravity="center_horizontal"
                android:paddingBottom="5dp"
                android:text="PREFACTURA"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="10sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relCamara"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:layout_below="@+id/relCanastas"
            android:layout_alignParentStart="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="5dp"
            android:onClick="tomarFoto">

            <ImageView
                android:id="@+id/imgCamara"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_alignParentStart="true"
                android:layout_marginStart="0dp"
                android:paddingEnd="6dp"
                android:paddingBottom="6dp"
                android:src="@drawable/camara" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imgCamara"
                android:layout_alignParentLeft="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="5dp"
                android:gravity="center_horizontal"
                android:paddingBottom="5dp"
                android:text="CÁMARA"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textSize="10sp"
                android:textStyle="bold"
                tools:visibility="visible" />

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relRoad"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">

        <RelativeLayout
            android:id="@+id/relRoadbar"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_centerVertical="true"
            android:background="#9A999E"></RelativeLayout>

        <ImageView
            android:id="@+id/imgTitLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp"
            android:adjustViewBounds="true"
            android:src="@drawable/logo_nuevo" />

        <ImageView
            android:id="@+id/imgRoadTit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/relRoadbar"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:adjustViewBounds="true"
            android:onClick="mostrarFachada"
            android:padding="5dp"
            android:src="@drawable/cliente" />

        <TextView
            android:id="@+id/txtRoadTit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imgRoadTit"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:text="Cliente"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#1A8AC6" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relbotpan"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="1dp"
        android:background="@layout/blue_strip">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:gravity="center_horizontal"
            android:paddingRight="10dp"
            android:text="DTSolutions"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/relbotpan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="3dp"
        android:layout_marginRight="3dp"
        android:padding="2dp">

        <ImageView
            android:id="@+id/imgPFoto"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:clickable="true"
            android:onClick="sendSMS"
            android:src="@drawable/whatsapp" />

        <ImageView
            android:id="@+id/imgImg"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:clickable="true"
            android:onClick="callPhone"
            android:src="@drawable/telefono" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_alignParentTop="false"
            android:layout_alignParentRight="true"
            android:adjustViewBounds="true"
            android:clickable="true"
            android:onClick="showDevol"
            android:src="@drawable/devolucion"
            android:visibility="visible" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="false"
            android:layout_toLeftOf="@+id/imgPFoto"
            android:layout_toRightOf="@+id/imgImg">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:clickable="true"
                android:onClick="callWaze"
                android:src="@drawable/waze" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView5"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:gravity="center_horizontal"
                android:text="Waze"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="10sp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView4"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="false"
            android:layout_toLeftOf="@+id/imageView1"
            android:layout_toRightOf="@+id/imgPFoto">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="false"
                android:layout_centerHorizontal="true"
                android:adjustViewBounds="true"
                android:clickable="true"
                android:onClick="showCredit"
                android:src="@drawable/cobro" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView2"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:gravity="center_horizontal"
                android:text="Cobro"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="10sp" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView1"
            android:layout_alignLeft="@+id/imageView1"
            android:layout_alignRight="@+id/imageView1"
            android:gravity="center_horizontal"
            android:text="Devolución"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imgPFoto"
            android:layout_alignParentBottom="false"
            android:layout_centerHorizontal="true"
            android:gravity="center_horizontal"
            android:paddingBottom="3dp"
            android:text="WhatsApp"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/lblSel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView4"
            android:layout_alignRight="@+id/imgImg"
            android:layout_alignParentLeft="true"
            android:gravity="center_horizontal"
            android:text="Teléfono"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:textSize="10sp" />

    </RelativeLayout>

</RelativeLayout>