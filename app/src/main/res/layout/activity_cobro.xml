<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@layout/back"
    tools:context=".Cobro" >

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView23"
        android:layout_alignParentStart="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="-5dp"
        android:background="@layout/frame_btn"
        android:clickable="true"
        android:onClick="sinRef"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:text="Sin Ref" />

    <ImageView
        android:id="@+id/imgImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:clickable="true"
        android:onClick="doExit"
        android:paddingBottom="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="5dp"
        android:src="@drawable/next48" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:src="@drawable/pay_any" />

    <TextView
        android:id="@+id/lblCVence"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/imageView2"
        android:layout_margin="5dp"
        android:layout_toRightOf="@+id/imageView1"
        android:background="@layout/frame_rect"
        android:clickable="true"
        android:gravity="center"
        android:onClick="paySelect"
        android:text="Pago"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageView2"
        android:layout_margin="5dp"
        android:background="@layout/frame_rect"
        android:clickable="true"
        android:gravity="center"
        android:onClick="payCash"
        android:text="Pago Efectivo"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="14sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/textView4"
        android:paddingBottom="5dp"
        android:src="@drawable/pay_cash" />

    <ListView
        android:id="@+id/listView1"
        android:layout_width="wrap_content"
        android:layout_height="447dp"
        android:layout_above="@+id/imageView1"
        android:layout_below="@+id/relLey"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="9dp">

    </ListView>

    <TextView
        android:id="@+id/lblCValor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:text="Seleccionado : "
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/lblCPago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/lblCValor"
        android:layout_below="@+id/lblCValor"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:text="Pagado :"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/lblCPago"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:text="A Pagar :"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#1A8AC6"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/lblSel"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/lblCValor"
        android:layout_alignBottom="@+id/lblCValor"
        android:layout_toRightOf="@+id/lblCValor"
        android:gravity="right"
        android:text="9,999,999.99"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/lblPag"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/lblCPago"
        android:layout_alignBottom="@+id/lblCPago"
        android:layout_toRightOf="@+id/lblCPago"
        android:gravity="right"
        android:text="9,999,999.99"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/lblPend"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toRightOf="@+id/lblCPago"
        android:gravity="right"
        android:text="9,999,999.99"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#1A8AC6"
        android:textStyle="bold" />

    <RelativeLayout
        android:id="@+id/relLey"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_below="@+id/textView23"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="4dp"
        android:layout_marginBottom="5dp"
        android:background="@layout/frame_rect">

        <TextView
            android:id="@+id/lblEPend"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="Medium Text"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/lblEEnv"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/lblEPend"
            android:gravity="right"
            android:paddingTop="5dp"
            android:text="Documento"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/lblEPend"
            android:layout_alignBottom="@+id/lblEPend"
            android:layout_toRightOf="@+id/lblEEnv"
            android:gravity="right"
            android:text="Valor orig."
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lblEEnv"
            android:layout_alignLeft="@+id/TextView01"
            android:gravity="right"
            android:text="Pago"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView03"
            android:layout_alignBottom="@+id/TextView03"
            android:layout_toLeftOf="@+id/TextView01"
            android:gravity="right"
            android:text="Saldo"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/TextView04"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/TextView01"
            android:layout_alignBottom="@+id/TextView01"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="Emitido"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/TextView05"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/lblEEnv"
            android:layout_alignBottom="@+id/TextView03"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="Vence"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="14sp" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView26"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="5dp"
        android:background="@layout/frame_btn"
        android:clickable="true"
        android:onClick="checkNone"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:text="Desmarcar todo"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="5dp"
        android:background="@layout/frame_btn"
        android:clickable="true"
        android:onClick="checkAll"
        android:paddingLeft="10dp"
        android:paddingTop="5dp"
        android:paddingRight="10dp"
        android:paddingBottom="5dp"
        android:text="Marcar todo"
        android:visibility="invisible" />

    <CheckBox
        android:id="@+id/cbCheckAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lblPend"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="48dp"
        android:checked="true"
        android:onClick="checkAll"
        android:text="Seleccionar Todo" />

</RelativeLayout>